{
	"name": "two-mi18n",
	"version": "0.1.0",
	"description": "Minimalist javascript library for internationalization in only two methods. Really minimalist.",
	"files": [
		"dist"
	],
	"main": "./dist/Mi18n.umd.js",
	"module": "./dist/Mi18n.es.js",
	"exports": {
		".": {
			"import": "./dist/Mi18n.es.js",
			"require": "./dist/Mi18n.umd.js"
		}
	},
	"scripts": {
		"dev": "vite",
		"build": "vite build",
		"preview": "vite preview"
	},
	"keywords": [
		"i18n",
		"translation",
		"translations"
	],
	"author": "Nicolas Renault https://nicolasrenault.com",
	"license": "MIT",
	"devDependencies": {
		"vite": "^5.0.11"
	}
}
